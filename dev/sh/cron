#/bin/bash

apt update > /tmp/cron.log 2> /tmp/cron.log
apt upgrade -y --no-install-recommends git curl nano sudo systemd bsdmainutils \
bash-completion cron ca-certificates iproute2 net-tools iptables-persistent apt-transport-https \
whiptail dnsutils procps grep dhcpcd5 iptables-persistent update > /tmp/cron.log 2> /tmp/cron.log
